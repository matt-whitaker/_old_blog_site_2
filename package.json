{
  "name": "disjointed-thinking",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "copy:ejs": "gulp copy:ejs",
    "compile:less": "gulp compile:less",
    "clean": "gulp clean && hexo clean",
    "download": "gulp retrieve:assets",
    "build": "npm run clean && npm run download && gulp build",
    "generate": "npm run build && hexo generate",
    "test:lint": "lesshint -c .lesshintrc src/less/",
    "test": "npm run test:lint",
    "server": "hexo server -p 80",
    "start": "npm run generate && chokidar 'src/**/*' -c 'npm run generate'",
    "deploy": "npm run generate && gulp deploy"
  },
  "hexo": {
    "version": "3.3.8"
  },
  "dependencies": {
    "dotenv": "^2.0.0",
    "gulp-watch": "^4.3.9",
    "hexo": "^3.2.0",
    "hexo-generator-archive": "^0.1.4",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-index": "^0.2.0",
    "hexo-generator-tag": "^0.2.0",
    "hexo-renderer-ejs": "^0.2.0",
    "hexo-renderer-marked": "^0.2.10",
    "hexo-renderer-stylus": "^0.3.1",
    "hexo-server": "^0.2.0",
    "mkdirp": "^0.5.1",
    "node-dropbox": "^0.1.7",
    "node-libcurl": "^1.0.0",
    "ramda": "^0.22.1",
    "request": "^2.74.0"
  },
  "devDependencies": {
    "aws-sdk": "^2.94.0",
    "chokidar-cli": "^1.2.0",
    "colors": "^1.1.2",
    "config": "^1.26.1",
    "del": "^2.2.1",
    "file-type": "^5.2.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-flatten": "^0.3.0",
    "gulp-less": "^3.1.0",
    "gulp-sequence": "^0.4.6",
    "gulp-sourcemaps": "^1.6.0",
    "html-comments": "^0.5.0",
    "lesshint": "^2.1.1",
    "request-promise": "^4.2.1"
  }
}
