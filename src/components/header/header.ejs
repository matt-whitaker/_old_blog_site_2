<% if (!page.suppress_header) { %>
    <header id="dt-header" style="background: url('<%- page.image %>'); <%- page.image_offset ? 'background-position: ' + page.image_offset : '' %>; <%- page.image_size ? 'background-size: ' + page.image_size : '' %>;">
        <h1><span><%- page.title -%></span></h1>
    </header>
<% } %>